                                                            "example": "ICD10DX",
                                                            "description": "the claim type, one of \"ICD9DX\", \"ICD10DX\", \"CPT\", \"HCPCS\", \"NDC9\", \"ICD9PX\", \"ICD10PX\" or \"DRG\"."
                                                        },
                                                        "code": {
                                                            "type": "string",
                                                            "example": "I10",
                                                            "description": "one of the top N claim codes that selected by machien learning algorithm supporting suspcted condition or the claim codes in target year directly indicating the known condition or the claim codes in prior years directly indicating historical known condition."
                                                        },
                                                        "provider_id": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "integer",
                                                                "example": 234,
                                                                "description": "the practitioner ID who reported this claim code."
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
