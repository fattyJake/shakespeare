                                    "example": 100001,
                                    "description": "the ID of patient/member/beneficiary."
                                },
                                "gaps": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "required": [
                                            "condition_category",
                                            "confidence",
                                            "known"
                                        ],
                                        "properties": {
                                            "condition_category": {
                                                "type": "string",
                                                "example": "HCC18",
                                                "description": "high-level CMS/HHS/CDPS defined conditions to be targeted."
                                            },
                                            "confidence": {
                                                "type": "number",
                                                "minimum": 0.0,
                                                "maximum": 1.0,
                                                "example": 0.830449,
                                                "description": "the confidence level of gap based on machine learning (probability between 0 to 1)."
                                            },
                                            "known": {
                                                "type": "integer",
                                                "enum": [
                                                    0,
                                                    1
                                                ],
                                                "example": 1,
                                                "description": "whether the condition is a known condition, i.e. there are at least one ICD10DX code directly indicating the condition based on CMS/HHS/CDPS model."
                                            },
                                            "top_indicators": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
                                                    "required": [
                                                        "code_type",
                                                        "code"
                                                    ],
                                                    "properties": {
                                                        "code_type": {
                                                            "type": "string",
                                                            "example": "ICD10DX",
                                                            "description": "the claim type, one of \"ICD9DX\", \"ICD10DX\", \"CPT\", \"HCPCS\", \"NDC9\", \"ICD9PX\", \"ICD10PX\" or \"DRG\"."
                                                        },
                                                        "code": {
                                                            "type": "string",
                                                            "example": "I10",
                                                            "description": "one of the top N claim codes that selected by machien learning algorithm supporting suspcted condition or the claim codes in prior years directly indicating the condition."
                                                        },
                                                        "provider_id": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "integer",
                                                                "example": 234,
                                                                "description": "the practitioner ID who reported this claim code."
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "prospective": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "required": [
                                "mem_id",
                                "gaps"
                            ],
                            "properties": {
                                "mem_id": {
                                    "type": "integer",
                                    "example": 100001,
                                    "description": "the ID of patient/member/beneficiary."
                                },
                                "gaps": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "required": [
                                            "condition_category",
                                            "confidence",
                                            "known_current",
                                            "known_historical"
                                        ],
                                        "properties": {
                                            "condition_category": {
                                                "type": "string",
                                                "example": "HCC18",
                                                "description": "high-level CMS/HHS/CDPS defined conditions to be targeted."
                                            },
                                            "confidence": {
                                                "type": "number",
                                                "minimum": 0.0,
                                                "maximum": 1.0,
                                                "example": 0.830449,
                                                "description": "the confidence level of gap based on machine learning (probability between 0 to 1)."
                                            },
                                            "known_current": {
                                                "type": "integer",
                                                "enum": [
                                                    0,
                                                    1
                                                ],
                                                "example": 1,
                                                "description": "whether the condition is already a known condition in the target year, i.e. there are at least one ICD10DX code in target year directly indicating the condition based on CMS/HHS/CDPS model."
                                            },
                                            "known_historical": {
                                                "type": "integer",
                                                "enum": [
                                                    0,
                                                    1
                                                ],
                                                "example": 0,
                                                "description": "whether the condition is known in prior service period, i.e. there are at least one ICD10DX code in prior year directly indicating the condition based on CMS/HHS/CDPS model."
                                            },
                                            "top_indicators": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
